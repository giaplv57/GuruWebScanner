rule GuruWS_SimpleShell
{
    strings:
        $ = /\<\?(.*|\n)system\(\$(.*)\]\)/
        $ = /\<\?(.*|\n)eval\(\$(.*)\]\)/
        $ = /\<\?(.*|\n)passthru\(\$(.*)\]\)/
        $ = /\<\?(.*|\n)shell_exec\(\$(.*)\]\)/
        $ = /\<\?(.*|\n)exec\(\$(.*)\]\)/
        $ = /\<\?(.*|\n)fread\(popen\(\$\_(.*)\]\, \'r\'\)\,\$\_(.*)\]\)/
        $ = /\<\?(.*|\n)pcntl_exec\(\'(.*)'\,array\(\'\-c\'\,\$\_(.*)\]\)\)/
        $ = /\<\?(.*|\n)preg_replace\(\'(.*)\'\,\$\_(.*)\],(.*)/
        $ = /\<\?(.*|\n)call_user_func_array\(\$\_(.*)\]\, array\(\$\_(.*)\]\)\)/
        $ = /\<\?(.*|\n)assert\(\$\_(.*)\]\)/        
        $ = /\<\?\=\@\$(.*)\(\$(.*)/
        $ = /\<\?\$(.*)\=str_replace\((.*)\)\;\@\$(.*)\(\$\_(.*)\]\)/
        $ = /\<\?\$(.*)\;\@\$(.*)\(\$\_(.*)\]\)/
        $ = /\<\?\$x\=strrev\(\"(.*)\"\)\;echo \@\$(.*)\(\$\_(.*)\]\)/
    condition:
        any of them
}

